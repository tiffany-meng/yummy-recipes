<html>
<head>
	<title>Home Page</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="css/stylesheet.css">
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>
<body>
	<h1 class="title">What's for Dinner?</h1>
	<div class="input-group rounded search">
		<input type="search" class="form-control rounded" placeholder="Search" aria-label="Search"
			aria-describedby="search-addon" />
		<span class="input-group-text border-0" id="search-addon">
			<i class="fas fa-search"></i>
		</span>
	</div>
	<div class="row ourrow">
		<div class="col-6">
			<div class="cuisine left">
				<a class="button" href="/cuisine/1">
					<div class="container1">
						<img class="image" src="images/american.jpg" alt="">	
						<div class="centered">American</div>
					</div>
				</a>
			</div>
		</div>
		<div class="col-6">
			<div class="cuisine right">
				<a class="button" href="/cuisine/2">
					<div class="container1">
						<img class="image" src="images/cajun.jpeg" alt="">	
						<div class="centered">Cajun</div>
					</div>
				</a>
			</div>
		</div>
	</div>
	<div class="row ourrow">
		<div class="col-6">
			<div class="cuisine left">
				<a class="button" href="/cuisine/3">
					<div class="container1">
						<img class="image" src="images/chinese.png" alt="">
						<div class="centered">Chinese</div>
					</div>
				</a>
			</div>
		</div>
		<div class="col-6">
			<div class="cuisine right">
				<a class="button" href="/cuisine/4">
					<div class="container1">
						<img class="image" src="images/indian.jpg" alt="">
						<div class="centered">Indian</div>
					</div>
				</a>
			</div>
		</div>
	</div>
	<div class="row ourrow">
		<div class="col-6">
			<div class="cuisine left">
				<a class="button" href="/cuisine/5">
					<div class="container1">
						<img class="image" src="images/italian.jpg" alt="">
						<div class="centered">Italian</div>
					</div>
				</a>
			</div>
		</div>
		<div class="col-6">
			<div class="cuisine right">
				<a class="button" href="/cuisine/6">
					<div class="container1">
						<img class="image" src="images/mexican.jpg" alt="">
						<div class="centered">Mexican</div>
					</div>
				</a>
			</div>
		</div>
	</div>

	<div class="row ourrow lastRow">
		<div class="col-6">
			<div class="cuisine left">
				<a class="button" href="/cuisine/7">
					<div class="container1">
						<img class="image" src="images/sushi.jpeg" alt="">
						<div class="centered">Japanese</div>
					</div>
				</a>
			</div>
		</div>
		<div class="col-6">
			<div class="cuisine right">
				<a class="button" href="/cuisine/8">
					<div class="container1">
						<img class="image" src="images/thai.jpg" alt="">
						<div class="centered">Thai</div>
					</div>
				</a>
			</div>
		</div>
	</div>


	<footer>
		<div class="footDiv">
			<button onclick="window.location.href='/'" type="button" class="btn btn-lg">
				<i class="fa fa-home"></i>
			</button>
			<button onclick="window.location.href='/library'" type="button" class="btn btn-lg">
				<i class="fas fa-heart"></i>
			</button>
			<button onclick="window.location.href='/search'" type="button" class="btn btn-lg">
				<i class="fas fa-search"></i>
			</button>
			<button onclick="window.location.href='/preferences'" type="button" class="btn btn-lg">
				<i class="fas fa-user"></i>
			</button>
		</div>
	</footer>

<!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
	<script src="https://use.fontawesome.com/releases/v5.15.2/js/all.js" data-auto-replace-svg="nest"></script>
</body>
</html>
